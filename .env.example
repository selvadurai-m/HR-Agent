# Supabase Configuration (REQUIRED for login)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Database Table Names
NEXT_PUBLIC_USERS_TABLE_NAME=users
NEXT_PUBLIC_INTERVIEWS_TABLE_NAME=interviews
NEXT_PUBLIC_INTERVIEW_RESULTS_TABLE_NAME=interview_results

# AI Service Keys (Required for interviews)
OPENROUTER_API_KEY=your_openrouter_api_key
VAPI_API_KEY=your_vapi_api_key

# Optional: Google AI Models (if using Google instead of OpenRouter)
GOOGLE_MODEL_QUESTION_GENERATION=gemini-1.5-pro
GOOGLE_MODEL_ANSWER_EVALUATION=gemini-1.5-flash
GOOGLE_MODEL_FEEDBACK=gemini-1.5-pro
GOOGLE_GEMINI_API_KEY=your_google_api_key

# OpenRouter Models (if using OpenRouter)
OPENROUTER_MODEL_QUESTION_GENERATION=deepseek/deepseek-r1:free
OPENROUTER_MODEL_ANSWER_EVALUATION=deepseek/deepseek-r1:free
OPENROUTER_MODEL_FEEDBACK=deepseek/deepseek-r1:free
